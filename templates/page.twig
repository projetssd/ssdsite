                            <img src="assets/images/logo-light-icon@2x.png" alt="homepage" width="76px"
                                class="light-logo" />
                        </b>
                        <!--End Logo icon -->
                        <!-- Logo text -->
                        <span class="logo-text">
                            <!-- Light Logo text -->
                            <img src="assets/images/logo-light-text@2x.png" class="light-logo" width="91px"
