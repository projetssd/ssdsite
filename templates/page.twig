                                <i class="mdi mdi-information font-24"></i>
                            </a>
                            {% include 'blocks/block_server.twig' %}
                            
                        </li>
                        <!-- ============================================================== -->
                        <!-- End mega menu -->
                        <!-- ============================================================== -->
                    </ul>
                    <!-- ============================================================== -->
