                        <!-- ============================================================== -->
                        <div class="dropdown-divider"></div>
                        <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark" href="install.html"><i
                                    class="mdi mdi-arrow-right-bold-circle"></i><span class="hide-menu install-modal">Installation</span></a></li>
                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
