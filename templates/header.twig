<!-- Custom CSS -->

{% if ASSET_CSS is empty %} 
    {# On est dans le mode dev, on affiche toutes les inclusions css #}
    {% for css in CSS %} 
        {% if css starts with 'http' %}
            <link rel="stylesheet" href="{{ css }}"> 
        {% else %}
            <link rel="stylesheet" href="dist/css/{{ css }}"> 
        {% endif %} 
    {% endfor %} 
    {% else %} 
    {# On est en mode prod, on affiche les assets css générés #}
    {{ ASSET_CSS|raw }} 
{% endif %} 
